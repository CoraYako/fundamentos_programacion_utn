Proceso ejercicio7_clase2b
	Definir numSemana Como Entero;
	Escribir Sin Saltar "Indique un número para saber qué día de la semana es:";
	Leer numSemana;
	Segun numSemana Hacer
		1:
			Escribir "Es Lunes!";
		2:
			Escribir "Es Martes!";
		3:
			Escribir "Es Miércoles!";
		4:
			Escribir "Es Jueves!";
		5:
			Escribir "Es Viernes!";
		6:
			Escribir "Es Sábado!";
		7:
			Escribir "Es Domingo!";
		De Otro Modo:
			Escribir "Día no encontrado";
	FinSegun
FinProceso
